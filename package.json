  "type": "commonjs",
  "scripts": {
    "start": "npm run build && node build/server.js",
    "dev": "nodemon -r dotenv/config src/server.ts",
    "build": "npm run lint && npm run clean && npm run build:ts",
    "build:ts": "tsc -p tsconfig.build.json",

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Driver {
  id      String    @id @default(uuid())
  name    String    @unique
}

model Car {
  id      String    @id @default(uuid())
  licensePlate String @unique
  color String
  brand String
}